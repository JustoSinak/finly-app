{
    "version": 2,
    "builds": [
      {
        "src": "tailwind.css",
        "use": "@vercel/static-build",
        "config": {
          "minify": true
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "env": {
        "NODE_ENV": "development"
      }
  }